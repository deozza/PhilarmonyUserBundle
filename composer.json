{
    "name": "deozza/philarmony-user-bundle",
    "description": "An user implementation adapted to Philarmony",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Edenn Touitou",
            "email": "deozza@gmail.com"
        }
